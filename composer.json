{
  "name": "laravelcm/laravelcm",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel",
    "community",
    "Cameroon"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "archtechx/laravel-seo": "^0.10.3",
    "awcodes/filament-badgeable-column": "^3.0.1",
    "codeat3/blade-phosphor-icons": "^2.3",
    "cyrildewit/eloquent-viewable": "^7.0.6",
    "diglactic/laravel-breadcrumbs": "^10.0",
    "filament/filament": "^4.5.1",
    "filament/spatie-laravel-media-library-plugin": "^4.5.1",
    "gehrisandro/tailwind-merge-laravel": "^1.3",
    "graham-campbell/markdown": "^16.0",
    "guzzlehttp/guzzle": "^7.10.0",
    "internachi/modular": "^2.3",
    "jenssegers/agent": "^2.6.4",
    "lara-zeus/spatie-translatable": "^1.0.4",
    "laravel-notification-channels/telegram": "^6.0",
    "laravel-notification-channels/twitter": "^8.3",
    "laravel/framework": "^12.46.0",
    "laravel/nightwatch": "^1.21.1",
    "laravel/octane": "^2.13.4",
    "laravel/socialite": "^5.24.1",
    "laravel/tinker": "^2.11.0",
    "laravelcm/database-migration": "^1.0",
    "laravelcm/gamify": "^1.1",
    "laravelcm/laravel-subscriptions": "^1.5",
    "laravelcm/livewire-slide-overs": "^1.0.8",
    "league/flysystem-aws-s3-v3": "^3.30.1",
    "livewire/flux": "^2.10.2",
    "livewire/flux-pro": "^2.10.2",
    "livewire/volt": "^1.10.1",
    "mckenziearts/filament-untitledui-icons": "^1.0",
    "mckenziearts/laravel-notify": "^3.1.1",
    "mckenziearts/livewire-markdown-editor": "^1.0.1",
    "notchpay/notchpay-php": "^2.0",
    "predis/predis": "^3.3",
    "ramsey/uuid": "^4.9.2",
    "shopperlabs/pureline-theme": "^1.0",
    "spatie/laravel-data": "^4.18",
    "spatie/laravel-feed": "^4.4.4",
    "spatie/laravel-permission": "^6.24.0",
    "spatie/laravel-sitemap": "^7.3.8",
    "stevebauman/location": "^7.6.1",
    "torchlight/torchlight-laravel": "^0.6.1",
    "wire-elements/spotlight": "^2.0.2",
    "yarri/link-finder": "^2.7.12",
    "ysfkaya/filament-phone-input": "^4.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.16.3",
    "driftingly/rector-laravel": "^2.1.9",
    "fakerphp/faker": "^1.24.1",
    "larastan/larastan": "^3.8.1",
    "laravel/boost": "^1.8.9",
    "laravel/breeze": "^2.3.8",
    "laravel/pail": "^1.2.4",
    "laravel/pint": "^1.27",
    "laravel/sail": "^1.52.0",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.8.3",
    "pestphp/pest": "^3.8.4",
    "pestphp/pest-plugin-laravel": "^3.2",
    "pestphp/pest-plugin-livewire": "^3.0",
    "pestphp/pest-plugin-type-coverage": "^3.6.1",
    "rector/rector": "^2.3.0",
    "spatie/test-time": "^1.3.3"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "dev:install": [
      "composer install",
      "./vendor/bin/sail up -d",
      "./vendor/bin/sail artisan octane:install --server=frankenphp",
      "./vendor/bin/sail npm install --no-update-notifier",
      "./vendor/bin/sail npm run build",
      "./vendor/bin/sail stop"
    ],
    "dev:uninstall": "docker compose down --rmi all --volumes --remove-orphans",
    "dev:run": [
      "Composer\\Config::disableProcessTimeout",
      "./vendor/bin/sail up -d",
      "./vendor/bin/sail npm config set update-notifier false",
      "./vendor/bin/sail npx concurrently -c \"#505b92,#d32d21\" \"composer install\" \"npm install --no-update-notifier\" --names=composer,npm",
      "./vendor/bin/sail npx concurrently -c \"#fb7185,#fdba74\" \"artisan pail --timeout=0\" \"npm run dev\" --names=logs,vite"
    ],
    "dev:stop": "./vendor/bin/sail down",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan clear",
      "@update:requirements",
      "./vendor/bin/sail artisan boost:update --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "update:requirements": [
      "composer bump",
      "npx npm-check-updates -u"
    ],
    "lint": [
      "rector",
      "pint --parallel",
      "npm run lint"
    ],
    "test:lint": [
      "pint --parallel --test",
      "rector --dry-run",
      "npm run test:lint"
    ],
    "test:unit": "pest --parallel --coverage --min=30.0",
    "test:types": "phpstan analyse --memory-limit=-1",
    "test:type-coverage": "pest --type-coverage --min=98 --memory-limit=-1",
    "test": [
      "@test:type-coverage",
      "@test:types",
      "@test:unit"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": false
    }
  },
  "repositories": [
    {
      "name": "flux-pro",
      "type": "composer",
      "url": "https://composer.fluxui.dev"
    },
    {
      "type": "path",
      "url": "app-modules/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "composer",
      "url": "https://pureline.composer.sh"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "stable"
}
